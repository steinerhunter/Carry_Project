<div class="edit_delivery_field_wrapper" style="background-image: url('../assets/LocationA.png');">
  <%= form_for(@request_delivery, :remote => true ) do |f| %>
      <div class="edit_delivery_field_from">
        <%= f.label :from, "From?" %><%= f.text_field :from  %>
      </div>
      <div class="edit_update">
        <%= f.submit "Update Request" , :id => 'submit_delivery' %>
      </div>
  <% end  %>
</div>
